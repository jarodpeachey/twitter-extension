{
 "manifest_version": 2,
 "name": "Material Twitter",
 "version": "1.0.0",
 "description": "This is a sample description",
 "short_name": "Material Twitter",
 "permissions": ["activeTab", "declarativeContent", "storage"],
 "content_scripts": [
   {
     "matches": ["https://twitter.com"],
     "css": ["override.css"],
     "js": ["background.js"]
   }
 ],
 "browser_action": {
   "default_title": "Customize Twitter",
   "default_popup": "popup.html",
   "default_icon": {
     "16": "icons/icon16.png",
     "32": "icons/icon32.png"
   }
 }
}
